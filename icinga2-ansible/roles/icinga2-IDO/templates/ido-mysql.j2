
library "db_ido_mysql"

object IdoMysqlConnection "ido-mysql" {
{% for key, value in icinga2.ido_connection.iteritems() %}
  {{ key }} = "{{ value }}"
{% endfor %}
{% if icinga2.ido_cleanup is defined %}
  cleanup = {
{% for key, value in icinga2.ido_cleanup.iteritems() %}
     {{ key }} = {{ value }}
{% endfor %}
  }
{% endif %}
}
